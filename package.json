{
  "name": "willhaben-agent",
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir . --ext .ts --ext .tsx",
    "cypress:open": "npx --no-install cypress open-ct",
    "lint:editorconfig": "npx --no-install ec .",
    "lint:package.json": "npmPkgJsonLint -c .npmpackagejsonlintrc.json .",
    "prepare": "husky install",
    "pre-commit": "npm-run-all -l -p lint lint:editorconfig lint:package.json"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.7",
    "@cypress/react": "^5.9.1",
    "@cypress/webpack-dev-server": "^1.4.0",
    "@types/react": "17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "babel-plugin-istanbul": "^6.0.0",
    "cypress": "^7.6.0",
    "editorconfig-checker": "^4.0.2",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-next": "11.0.1",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.23.4",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.1",
    "npm-package-json-lint": "^5.1.0",
    "npm-package-json-lint-config-default": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "4.3.4",
    "webpack-dev-server": "^3.11.2"
  },
  "engines": {
    "node": "^16"
  },
  "private": true
}
